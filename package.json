  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run compile-all && npm run watch-all",
    "watch-all": "concurrently -p \"[{name}]\" -n \"SRC,STYLES\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm:watch-ts\" \"npm:watch-scss\"",
    "watch-ts": "watch \"npm run compile-ts\" src/",
    "watch-scss": "watch \" npm run compile-scss\" styles/",
    "compile-scss": "node-sass styles/ -o dist/",
    "compile-ts": "webpack",
    "compile-all": "npm run compile-scss & npm run compile-ts"
  }
